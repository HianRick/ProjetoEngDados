    {
      "name": "590a85e1",
      "displayName": "GRAFICO_1",
      "queryLines": [
        "\r\n",
        "SELECT \r\n",
        "    ANO,\r\n",
        "    TIPO_VINHO,\r\n",
        "    SUM(VALOR_VENDIDO) AS VALOR_VENDIDO_TOTAL\r\n",
        "FROM \r\n",
        "    gold_vinhos.fato_vendas_concluidas\r\n",
        "GROUP BY \r\n",
        "    ANO, \r\n",
        "    TIPO_VINHO\r\n",
        "ORDER BY \r\n",
        "    ANO DESC, \r\n",
        "    VALOR_VENDIDO_TOTAL DESC"
      ]
    },
    {
      "name": "0e4a914f",
      "displayName": "GRAFICO_2",
      "queryLines": [
        "SELECT \r\n",
        "    SUM(QUANTIDADE_VENDIDA) AS total_vendido_2025\r\n",
        "FROM workspace.gold_vinhos.fato_vendas_concluidas\r\n",
        "WHERE ANO = 2025;\r\n"
      ]
    },
    {
      "name": "ed866ec1",
      "displayName": "GRAFICO_3",
      "queryLines": [
        "SELECT \r\n",
        "    SUM(VALOR_VENDIDO) AS total_vendido_2025\r\n",
        "FROM workspace.gold_vinhos.fato_vendas_concluidas\r\n",
        "WHERE ANO = 2025 AND TIPO_VINHO = 'Rosé';"
      ]
    },
    {
      "name": "e3650732",
      "displayName": "GRAFICO_4",
      "queryLines": [
        "SELECT \r\n",
        "    SUM(VALOR_VENDIDO) AS total_vendido_2025\r\n",
        "FROM workspace.gold_vinhos.fato_vendas_concluidas\r\n",
        "WHERE ANO = 2025 AND TIPO_VINHO = 'Espumante';"
      ]
    },
    {
      "name": "2dab71a7",
      "displayName": "GRAFICO_5",
      "queryLines": [
        "SELECT \r\n",
        "    SUM(QUANTIDADE_VENDIDA) AS total_vendido_2025\r\n",
        "FROM workspace.gold_vinhos.fato_vendas_concluidas\r\n",
        "WHERE PAIS = 'França' AND ANO = 2025;"
      ]
    },
    {
      "name": "0e7f7afc",
      "displayName": "GRAFICO_6",
      "queryLines": [
        "SELECT\r\n",
        "    TIPO_VINHO,\r\n",
        "    SUM(VALOR_VENDIDO) AS total_vendas,\r\n",
        "    SUM(VALOR_VENDIDO) * 100.0 / SUM(SUM(VALOR_VENDIDO)) OVER() AS percentual_vendas\r\n",
        "FROM workspace.gold_vinhos.fato_vendas_concluidas\r\n",
        "WHERE ano = 2025\r\n",
        "GROUP BY TIPO_VINHO\r\n",
        "ORDER BY total_vendas DESC;\r\n"
      ]
    }
  ],
